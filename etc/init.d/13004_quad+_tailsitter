#!nsh
#
# @name Quadrotor + Tailsitter
#
# @type VTOL Quad Tailsitter
#
# @maintainer Roman Bapst <roman@px4.io>
#

source /fs/microsd/etc/init.d/rc.vtol_defaults

if [ $AUTOCNF = yes ]
then
    param set VT_MOT_COUNT 4
    param set VT_IDLE_PWM_MC  1080
    param set VT_TYPE 0
    param set VT_ELEV_MC_LOCK 1
fi

export MIXER=quad_+_vtol

export PWM_OUT=1234
export PWM_MAX=2000
export PWM_RATE=400
export MAV_TYPE=20
